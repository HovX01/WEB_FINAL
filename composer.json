{
  "name": "jeffreyway/demo",
  "authors": [
    {
      "name": "Jeffrey Way",
      "email": "jeffrey@jeffrey-way.com"
    }
  ],
  "require": {
    "illuminate/collections": "^10.9",
    "ext-pdo": "*",
    "illuminate/database": "^10.48",
    "illuminate/events": "^10.48",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "Core\\": "Core/",
      "Http\\": "Http/",
      "Database\\Migrations\\": "Database/migrations"
    }
  },
  "require-dev": {
    "pestphp/pest": "^2.5"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
